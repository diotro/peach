{
  "name": "@peach/frontend",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "GPL-3.0",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "build": "parcel build src/index.html --public-url /assets",
